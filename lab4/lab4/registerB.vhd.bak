LIBRARY ieee;
USE ieee.std_logic_1164.ALL

ENTITY registerB IS 
	GENERIC (n: INTEGER := 8);
	PORT (w : IN std_logic(1 DOWNTO 0);
			clock : IN std_logic;
			Q : OUT std_logic_vector((n-1) DOWNTO 0)
			);
END registerB;
